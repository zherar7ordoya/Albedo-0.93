{
    "name": "Gray CLC",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        // Verdes suaves: tipos
        "class": "#28867F",
        "delegate": "#28867F",
        "recordClass": "#28867F",
        "type": "#28867F",
        // Azules cálidos y sobrios: palabras clave, namespaces, macros
        "keyword": "#3B6BAA",
        "namespace": "#3B6BAA",
        "macro": "#3B6BAA",
        "xmlDocCommentName": "#3B6BAA",
        // Verde oliva apagado para texto de comentarios XML
        "xmlDocCommentText": "#5B763B",
        // Verde más oscuro para comentarios regulares
        "comment": "#476B3D",
        // Gris medio para delimitadores XML
        "xmlDocCommentDelimiter": "#6A6A6A",
        // Verde pálido más diferenciado para structs
        "recordStruct": "#5F8F5B",
        "struct": "#5F8F5B",
        // Azul claro para variables, eventos, parámetros
        "event": "#367C9D",
        "parameter": "#367C9D",
        "variable": "#367C9D",
        // Verde pálido para números
        "number": "#739766",
        // Tipos especiales con verde hierba más vivo
        "interface": "#6C9745",
        "typeParameter": "#6C9745",
        "enum": "#6C9745",
        // Rosado lavanda para keywords de control
        "controlKeyword": "#8F5E8B",
        // Gris claro, útil como fallback visual o labels
        "label": "#7F7F7F",
        // Rojizo suave, clásico para strings y preprocesadores
        "stringVerbatim": "#A85A44",
        "preprocessorText": "#A85A44",
        "string": "#A85A44",
        // Gris oscuro neutro para lo accesorio: operadores, etc.
        "operator": "#4C4C4C",
        "punctuation": "#4C4C4C",
        "constant": "#4C4C4C",
        "enumMember": "#4C4C4C",
        "field": "#4C4C4C",
        "property": "#4C4C4C",
        // Amarillo más sobrio y legible para funciones
        "function": "#AA9830",
        "method": "#AA9830",
        "extensionMethod": "#AA9830"
    },
