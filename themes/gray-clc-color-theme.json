        "xmlDocCommentAttributeQuotes": "#005661",
        //
        "extensionMethod": "#2255AA",
        "function": "#2255AA",
        "method": "#2255AA",
        //
        "markupAttributeValue": "#006611",
        "preprocessorText": "#006611",
        "string": "#006611",
        "stringVerbatim": "#006611",
        //
        "constant": "#3A3C44",
        "event": "#3A3C44",
        "field": "#3A3C44",
        "label": "#3A3C44",
        "macro": "#3A3C44",
        "operator": "#3A3C44",
        "punctuation": "#3A3C44",
        "variable": "#3A3C44",
