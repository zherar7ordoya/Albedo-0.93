{
    // "name": "Optima Clara",
    "name": "Gray CLC",
    "type": "light",
    "semanticHighlighting": true,
    //
    //
    "semanticTokenColors": {
        "modifier": "#0033CC",
        "label": "#0000AA",
        "namespace": "#0000AA",
        "delegate": "#0000AA",
        "typeParameter": "#0000AA",
        "controlKeyword": "#8B008B",
        "keyword": "#8B008B",
        "enum": "#006400",
        "event": "#006400",
        "recordClass": "#006400",
        "class": "#006400",
        "property": "#006400",
        "string": "#006400",
        "method": "#076678",
        "function": "#076678",
        "extensionMethod": "#076678",
        "operator": "#076678",
        "comment": "#555555",
        "preprocessorText": "#555555",
        "xmlDocCommentText": "#555555",
        "regexp": "#880000",
        "stringVerbatim": "#880000",
        "number": "#880000",
        "enumMember": "#000000",
        "macro": "#000000",
        "field": "#000000",
        "constant": "#000000",
        "parameter": "#000000",
        "variable": "#000000",
        /* ************************** */
        // ROJO OSCURO
        // AZUL CLARO (mejorado)
        "xmlDocCommentName": "#1E4A7C",
        "xmlDocCommentAttributeName": "#1E4A7C",
        // VERDE OSCURO
        "interface": "#1E4A7C",
        "struct": "#004B73",
        "recordStruct": "#004B73",
        // GRIS CLARO
        "xmlDocCommentDelimiter": "#5E5E5E",
        "punctuation": "#5E5E5E",
        "decorator": "#FF0000",
        "type": "#FF0000",
    },
    /*
    Paleta de colores:
    - Azul:    #4078F2
    - Verde:   #008000
    - Gris:    #808080
    - Púrpura: #A626A4
    - Naranja: #C18401
    - Bordó:   #A31515

    Fondos:
    - Peach:  RGB(237, 221, 110) #EDD1B0; 14.35:1. 
    - Orange: RGB(216, 211, 214) #EDDD6E; 15.17:1. 
    - Yellow: RGB(248, 253, 137) #F8FD89; 19.4:1. 

    Texto predeterminado:
    - Negro:   #000000
    */
    "tokenColors": [
        // === COLOR: Azul -------------
        {
            "scope": [
                "entity.name.function",
                "keyword.operator",
                "punctuation.definition.interpolation",
                "punctuation.definition.string",
                "punctuation.definition.template-expression",
                "storage.type.function",
                "template-expression",
            ],
            "settings": {
                //"foreground": "#4078F2"
                //"foreground": "#0056B3"
                "foreground": "#0055DD"
            }
        },
        // === COLOR: Negro ------------
        {
            "scope": [
                "entity.name.function.member",
                "entity.name.function.preprocessor",
                "entity.name.namespace",
                "entity.name.variable",
                "punctuation",
                "punctuation.separator.hash",
                "support.constant",
                "support.function",
                "variable.other",
                "variable.parameter",
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        // === COLOR: Verde ------------
        {
            "scope": [
                

                "entity.name.type",
                "entity.name.type.class",
                "entity.name.type.enum",
                "entity.name.type.interface",
                "entity.other.inherited-class",
                "support.class",


            ],
            "settings": {
                //"foreground": "#008000"
                "foreground": "#008000"
                
            }
        },
        // === COLOR: Gris -------------
        {
            "scope": [
                "comment",
                "entity.name.tag.localname",
                "keyword.preprocessor.endregion",
                "keyword.preprocessor.region",
                "punctuation.definition.comment",
            ],
            "settings": {
                //"foreground": "#808080"
                "foreground": "#757575"
            }
        },
        // === COLOR: Púrpura ----------
        {
            "scope": [
                "keyword.control",
                "keyword.operator.expression",
                "keyword.other",
                "keyword.type",
                "keyword.preprocessor",
                "storage",
                "storage.type.struct",
                "support.type",
                "variable.language",
            ],
            "settings": {
                /*
                - Púrpura opción 1: #9900CC  (más saturado, matiz más intenso)
                - Púrpura opción 2: #A000D9  (más brillante, menos oscuro)
                - Púrpura opción 3: #8A00C4  (matiz más azulado, más vibrante)
                - Púrpura opción 4: #9D00B8  (más rojizo, contraste con el azul más azul)
                */
                "foreground": "#9900CC"
            }
        },
        // === COLOR: Naranja ----------
        {
            "scope": [
                "constant",
                "entity.other.attribute-name.localname",
                "support.variable",
                
            ],
            "settings": {
                //"foreground": "#C18401"
                //"foreground": "#C45500"
                //"foreground": "#B45F04"
                "foreground": "#7D4E00"
            }
        },
        // === COLOR: Rojo -------------
        {
            "scope": [
                
                "string",

            ],
            "settings": {
                //"foreground": "#A31515"
                //"foreground": "#8B0000"
                "foreground": "#DC143C", 
            }
        },
        // =============================
    ],
    /*
    Paleta de colores:
    - Azul:    #4078F2
    - Verde:   #008000
    - Gris:    #808080
    - Púrpura: #A626A4
    - Naranja: #C18401
    - Bordó:   #A31515

    Fondos:
    - Peach:  RGB(237, 221, 110) #EDD1B0; 14.35:1. 
    - Orange: RGB(216, 211, 214) #EDDD6E; 15.17:1. 
    - Yellow: RGB(248, 253, 137) #F8FD89; 19.4:1. 

    Texto predeterminado:
    - Negro:   #000000
    */    
    "colors": {
        "editor.background": "#EDD1B0",
        "editor.foreground": "#000000",
