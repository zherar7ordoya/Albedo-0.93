{
    // "name": "Optima Clara",
    "name": "Gray CLC",
    "type": "light",
    "semanticHighlighting": true,
    //
    /* ver https://code.visualstudio.com/api/language-extensions/semantic-highlight-guide */
//

    "semanticTokenColors": {
        "modifier": "#0033CC",
        "label": "#0000AA",
        "namespace": "#0000AA",
        "delegate": "#0000AA",
        "typeParameter": "#0000AA",
        "controlKeyword": "#8B008B",
        "keyword": "#8B008B",
        "enum": "#006400",
        "event": "#006400",
        "recordClass": "#006400",
        "class": "#006400",
        "property": "#006400",
        "string": "#006400",
        "method": "#076678",
        "function": "#076678",
        "extensionMethod": "#076678",
        "operator": "#076678",
        "comment": "#555555",
        "preprocessorText": "#555555",
        "xmlDocCommentText": "#555555",
        "regexp": "#880000",
        "stringVerbatim": "#880000",
        "number": "#880000",
        "enumMember": "#000000",
        "macro": "#000000",
        "field": "#000000",
        "constant": "#000000",
        "parameter": "#000000",
        "variable": "#000000",
        /* ************************** */
        // ROJO OSCURO
        // AZUL CLARO (mejorado)
        "xmlDocCommentName": "#1E4A7C",
        "xmlDocCommentAttributeName": "#1E4A7C",
        // VERDE OSCURO
        "interface": "#1E4A7C",
        "struct": "#004B73",
        "recordStruct": "#004B73",
        // GRIS CLARO
        "xmlDocCommentDelimiter": "#5E5E5E",
        "punctuation": "#5E5E5E",
        "decorator": "#FF0000",
        "type": "#FF0000",
    },
    //
    "tokenColors": [
        
        // =============================
        {
            "scope": [
                "support.type",
                "keyword.operator",
                
                "entity.name.function",
                
                "variable.other.object.property",

                "template-expression",

                
                "punctuation.definition.template-expression",
                "punctuation.definition.interpolation",

            ],
            "settings": {
                "foreground": "#0184BC"
            }
        },
        // -----------------------------
        {
            "scope": [

                "entity.name.function.member",
                "entity.name.function.preprocessor",
                "entity.name.namespace",
                
                "entity.name.variable",

                "keyword.preprocessor.endregion",
                "keyword.preprocessor.region",

                "punctuation",
                
                "support.function",
                "support.constant",
                
                "variable.other.constant",
                "variable.other.constant.property",
                "variable.other.enummember",
                "variable.other.event",
                "variable.other.object",
                "variable.other.property",
                "variable.other.readwrite",

                "variable.parameter",

        ],
            "settings": {
                "foreground": "#000000"
            }
        },
        // -----------------------------
        {
            "scope": [],
            "settings": {
                "foreground": "#4078F2"
            }
        },
        // -----------------------------
        {
            "scope": [
                

                //"entity.name.tag.localname",

                "entity.name.type",
                "entity.name.type.class",
                "entity.name.type.enum",
                "entity.name.type.interface",

                "entity.other.inherited-class",

                "punctuation.definition.string",
                
                "string",

                "support.class",

                

            ],
            "settings": {
                "foreground": "#50A14F"
            }
        },
        // -----------------------------
        {
            "scope": [
                "constant",
                "entity.other.attribute-name.localname",
            ],
            "settings": {
                "foreground": "#986801"
            }
        },
        // -----------------------------
        {
            "scope": [
                "comment",

                

                "punctuation.definition.comment",
                "punctuation.separator.hash",
                
                //"string.unquoted.preprocessor.message",
            ],
            "settings": {
                "foreground": "#A0A1A7"
            }
        },
        // -----------------------------
        {
            "scope": [
                
                "keyword.control",
                "keyword.other",
                "keyword.type",
                "keyword.preprocessor",
                
                "storage",
                "storage.type.struct",
                
                "variable.language",
            ],
            "settings": {
                "foreground": "#A626A4"
            }
        },
        // -----------------------------
        {
            "scope": [],
            "settings": {
                "foreground": "#C18401"
            }
        },
        // -----------------------------
        {
            "scope": [],
            "settings": {
                "foreground": "#CA1243"
            }
        },
        // -----------------------------
        {
            "scope": [
                "support.variable",
            ],
            "settings": {
                "foreground": "#E45649"
            }
        },
        // =============================
    ],
    "colors": {
        //"editor.background": "#F2E3CC",
        "editor.background": "#FAFAFA",
        "editor.foreground": "#000000",
        // =============================
