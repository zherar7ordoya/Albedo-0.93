{
    "name": "albedo-0-93",
    "displayName": "Albedo 0.93",
    "description": "A carefully tuned light theme based on Atom One Light. Albedo 0.93 explores the lower limit of background brightness while preserving clarity, contrast, and color accuracy. The name refers to the approximate reflectance of its background (#eeeeee), chosen as a compromise between readability and visual comfort during long coding sessions. WCAG-compliant and thoughtfully restrained.",
    "version": "0.9.0",
    "publisher": "Gerardo-Tordoya",
    "icon": "icon.png",
    "keywords": [
        "theme",
        "color theme",
        "light theme",
        "vscode",
        "accessibility",
        "wcag",
        "atom one light",
        "low contrast",
        "soft light",
        "designer-friendly",
        "minimal",
        "readability",
        "ui design",
        "super silver",
        "albedo",
        "contrast"
    ],
    "engines": {
        "vscode": "^1.102.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zherar7ordoya/Albedo-0.93.git"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Albedo 0.93",
                "uiTheme": "vs",
                "path": "./themes/Albedo 0.93-color-theme.json"
            }
        ],
        "configurationDefaults": {
            "[Albedo 0.93]": {
                "workbench.colorCustomizations": {
                    "terminal.background": "#2B2B2B",
                    "terminal.foreground": "#E6E1DC",
                    "terminalCursor.background": "#E6E1DC",
                    "terminalCursor.foreground": "#E6E1DC",
                    "terminal.ansiBlack": "#2B2B2B",
                    "terminal.ansiBrightBlack": "#5A647E",
                    "terminal.ansiRed": "#DA4939",
                    "terminal.ansiBrightRed": "#DA4939",
                    "terminal.ansiGreen": "#A5C261",
                    "terminal.ansiBrightGreen": "#A5C261",
                    "terminal.ansiYellow": "#FFC66D",
                    "terminal.ansiBrightYellow": "#FFC66D",
                    "terminal.ansiBlue": "#6D9CBE",
                    "terminal.ansiBrightBlue": "#6D9CBE",
                    "terminal.ansiMagenta": "#B6B3EB",
                    "terminal.ansiBrightMagenta": "#B6B3EB",
                    "terminal.ansiCyan": "#519F50",
                    "terminal.ansiBrightCyan": "#519F50",
                    "terminal.ansiWhite": "#E6E1DC",
                    "terminal.ansiBrightWhite": "#F9F7F3"
                }
            }
        }
    },
    "homepage": "https://github.com/zherar7ordoya/Albedo-0.93.git#readme"
}