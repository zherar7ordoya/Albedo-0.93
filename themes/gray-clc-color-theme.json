{
    "name": "Gray CLC",
    "type": "light",
    "tokenColors": [
        // MOSTAZA NORMAL
        {
            "scope": "entity.name.function.ts",
            "settings": {
                "foreground": "#996600"
            }
        },
        // VIOLETA NORMAL
        {
            "scope": "keyword.control.import.ts",
            "settings": {
                "foreground": "#7A3E9D"
            }
        },
        // GRIS ITALIC
        {
            "scope": "comment.line.double-slash.ts",
            "settings": {
                "foreground": "#777777"
            }
        },
        // VERDE NORMAL
        {
            "scope": "support.type.primitive.ts",
            "settings": {
                "foreground": "#007755"
            }
        },
        {
            "scope": "storage.type.function.arrow.ts",
            "settings": {
                "foreground": "#007755"
            }
        },
        // AZUL REAL
        {
            "scope": "variable.other.object.ts",
            "settings": {
                "foreground": "#000066"
            }
        },
        {
            "scope": "variable.language.this.ts",
            "settings": {
                "foreground": "#000066"
            }
        },
        {
            "scope": "variable.other.object.property.ts",
            "settings": {
                "foreground": "#000066"
            }
        },
        // AZUL REAL + ITALIC
        {
            "scope": "variable.other.readwrite.ts",
            "settings": {
                "foreground": "#000066",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "variable.parameter.ts",
            "settings": {
                "foreground": "#000066",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Namespaces",
            "scope": [
                "entity.name.namespace",
                "entity.name.type.namespace"
            ],
            "settings": {
                "foreground": "#000000"
            }
        }
    ],
