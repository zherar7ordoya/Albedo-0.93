{
    "name": "Gray CLC",
    // =================================
    "type": "light",
    // =================================
    "semanticHighlighting": true,
    // =================================
    "semanticTokenColors": {
        // Unused **********************
        "decorator": "#000000",
        "modifier": "#000000",
        "regexp": "#000000",
        "type": "#000000",
        // *****************************
        //
        /*
        Color Palette:
        - #0184BC
        - #383A42
        - #4078F2
        - #50A14F
        - #986801
        - #A0A1A7
        - #A626A4
        - #C18401
        - #E45649
        */
        //
        "operator": "#0184BC",
        "punctuation": "#0184BC",
        "xmlDocCommentAttributeQuotes": "#0184BC",
        //
        "constant": "#383A42",
        "enumMember": "#383A42",
        "event": "#383A42",
        "label": "#383A42",
        "macro": "#383A42",
        "variable": "#383A42",
        
        //
        "extensionMethod": "#4078F2",
        "function": "#4078F2",
        "method": "#4078F2",
        //      
        "parameter": "#50A14F",
        "preprocessorText": "#50A14F",
        "string": "#50A14F",
        "stringVerbatim": "#50A14F",
        //
        "number": "#986801",
        "xmlDocCommentAttributeName": "#986801",
        //
        "comment": "#A0A1A7",
        "xmlDocCommentDelimiter": "#A0A1A7",
        "xmlDocCommentText": "#A0A1A7",
        //      
        "controlKeyword": "#A626A4",
        "keyword": "#A626A4",
        //
        "class": "#C18401",
        "delegate": "#C18401",
        "enum": "#C18401",
        "interface": "#C18401",
        "namespace": "#C18401",
        "recordClass": "#C18401",
        "recordStruct": "#C18401",
        "struct": "#C18401",
        "typeParameter": "#C18401",
        
        //
        "field": "#E45649",
        "property": "#E45649",
        "xmlDocCommentName": "#E45649",
        //
    },
    // =================================
    "tokenColors": [
        {
            "scope": [],
            "settings": {
                "foreground": "#0184BC"
            }
        },
        {
            "scope": [
                "punctuation.definition.tag",
            ],
            "settings": {
                "foreground": "#383A42"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#4078F2"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#50A14F"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#986801"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#A0A1A7"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#A626A4"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#C18401"
            }
        },
        {
            "scope": [],
            "settings": {
                "foreground": "#E45649"
            }
        },
    ],
    // ========================= #FDF6E3
    "colors": {
        // base color
        "focusBorder": "#f0f0f000",
        "foreground": "#000000",
        "editor.background": "#FAFAFA", // <== base color
        "editor.foreground": "#383A42",
        "scrollbar.shadow": "#f0f0f0",
        // activity bar
        "activityBar.border": "#f0f0f0",
        "activityBar.background": "#f0f0f0",
        "activityBar.foreground": "#000000",
        "activityBarBadge.background": "#d73a49",
        // status bar
        "statusBar.border": "#f0f0f0",
        "statusBar.background": "#f0f0f0",
        "statusBar.foreground": "#000000",
        "statusBar.noFolderBackground": "#f0f0f0",
        "statusBar.noFolderForeground": "#000000",
        "statusBar.debuggingBackground": "#f0f0f0",
        "statusBar.debuggingForeground": "#000000",
        // editor groups and tabs
        "editorGroup.border": "#f0f0f0",
        "editorGroupHeader.tabsBackground": "#f0f0f0",
        "editorGroupHeader.noTabsBackground": "#f0f0f0",
        "editorGroupHeader.tabsBorder": "#f0f0f0",
        "tab.activeBackground": "#f0f0f0",
        "tab.inactiveBackground": "#f0f0f0",
        "tab.border": "#f0f0f0",
        "tab.activeBorder": "#d73a49",
        // side bars
        "sideBar.border": "#f0f0f0",
        "sideBar.background": "#f0f0f0",
        "sideBar.foreground": "#000000",
        "sideBarSectionHeader.background": "#f0f0f0",
        // list
        "list.highlightForeground": "#d73a49",
        "list.activeSelectionBackground": "#dfdfdf",
        "list.activeSelectionForeground": "#d73a49",
        "list.inactiveSelectionBackground": "#dfdfdf",
        "list.inactiveSelectionForeground": "#d73a49",
        "list.hoverBackground": "#dfdfdf",
        "list.hoverForeground": "#d73a49",
        "list.focusBackground": "#dfdfdf",
        "list.focusForeground": "#d73a49",
        // editor
        "editor.lineHighlightBackground": "#f0ecd0",
        "editor.lineHighlightBorder": "#f0ecd0",
        "editorLineNumber.foreground": "#babbbc",
        "editorLineNumber.activeForeground": "#000000",
        "editor.selectionBackground": "#fed442",
        // input
        "input.border": "#b2b2b2",
        "input.background": "#f0f0f0",
        "inputOption.activeBorder": "#000000",
        "inputOption.activeForeground": "#000000",
        "dropdown.border": "#b2b2b2",
        "dropdown.background": "#f0f0f0",
        "dropdown.listBackground": "#f0f0f0",
        // notification
        "notificationCenter.border": "#f0f0f0",
        "notificationCenterHeader.background": "#f0f0f0",
        "notificationToast.border": "#f0f0f0",
        "notifications.background": "#f0f0f0",
        "notifications.border": "#f0f0f0",
        // button
        "button.background": "#d73a49",
        "button.foreground": "#f0f0f0",
        // titlebar
        "titleBar.border": "#f0f0f0",
        "titleBar.activeBackground": "#f0f0f0",
        "titleBar.activeForeground": "#000000",
        "titleBar.inactiveBackground": "#f0f0f0",
        "titleBar.inactiveForeground": "#000000",
        // editor widget
        "editorWidget.background": "#f0f0f0",
        "editorWidget.border": "#000000",
        "editorSuggestWidget.highlightForeground": "#d73a49",
        "editorSuggestWidget.selectedBackground": "#e1e1e1",
        // panel
        "panel.border": "#d73a49"
    },
}