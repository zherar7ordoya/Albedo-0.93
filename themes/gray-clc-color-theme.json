            "settings": {
                "foreground": "#5A5A5A"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other.caps",
                "entity.name.type",
                "entity.other",
                "keyword.other.unit",
                "punctuation.definition.constant",
                "variable.parameter.function",
