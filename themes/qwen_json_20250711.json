{
  "name": "HighLight",
  "type": "light",
  "colors": {
    "focusBorder": "#00000000",
    "foreground": "#1A1A1A",
    "descriptionForeground": "#555555",
    "errorForeground": "#B22222",

    "button.background": "#2D6CB2",
    "button.foreground": "#FFFFFF",

    "titleBar.activeBackground": "#CCCCCC",
    "titleBar.activeForeground": "#1A1A1A",
    "titleBar.inactiveBackground": "#DDDDDD",
    "titleBar.inactiveForeground": "#555555",

    "activityBar.background": "#DDDDDD",
    "activityBar.foreground": "#1A1A1A",
    "activityBar.inactiveForeground": "#777777",

    "sideBar.background": "#E0E0E0",
    "sideBar.foreground": "#1A1A1A",
    "sideBar.border": "#CCCCCC",

    "menu.background": "#E0E0E0",
    "menu.foreground": "#1A1A1A",

    "editor.background": "#E0E0E0",
    "editor.foreground": "#1A1A1A",
    "editorLineNumber.foreground": "#555555",
    "editorLineNumber.activeForeground": "#1A1A1A",
    "editor.selectionBackground": "#CCCCCC",
    "editor.wordHighlightBackground": "#EEEEEE",
    "editorCursor.foreground": "#1A1A1A",
    "editor.lineHighlightBorder": "#CCCCCC",
    "editorIndentGuide.background": "#CCCCCC",
    "editorWhitespace.foreground": "#BBBBBB",
    "editor.findMatchBackground": "#FFD70088",
    "editor.findMatchHighlightBackground": "#FFD700AA",
    "editor.hoverHighlightBackground": "#EEEEEE"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#555555" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#2E8B57" }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": { "foreground": "#B22222" }
    },
    {
      "scope": ["constant.language.boolean", "constant.numeric"],
      "settings": { "foreground": "#007B4F" }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": { "foreground": "#2D6CB2" }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#CC5500" }
    },
    {
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": { "foreground": "#8C4FFF" }
    },
    {
      "scope": ["variable.constant", "constant.other"],
      "settings": { "foreground": "#8C4FFF" }
    },
    {
      "scope": ["enumMember"],
      "settings": { "foreground": "#8C4FFF" }
    },
    {
      "scope": ["variable.defaultLibrary"],
      "settings": { "foreground": "#007B5F" }
    },
    {
      "scope": ["operator", "keyword.operator"],
      "settings": { "foreground": "#333333" }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": { "foreground": "#333333" }
    },
    {
      "scope": ["meta.embedded"],
      "settings": { "foreground": "#1A1A1A" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#2D6CB2", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.quote"],
      "settings": { "foreground": "#555555" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "foreground": "#1A1A1A", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": { "foreground": "#B22222" }
    },
    {
      "scope": ["invalid.illegal"],
      "settings": { "foreground": "#FFFFFF", "background": "#B22222" }
    }
  ],
  "semanticTokenColors": {
    "namespace": { "foreground": "#1A1A1A" },
    "type": { "foreground": "#8C4FFF" },
    "class": { "foreground": "#8C4FFF" },
    "enum": { "foreground": "#8C4FFF" },
    "interface": { "foreground": "#8C4FFF" },
    "struct": { "foreground": "#8C4FFF" },
    "typeParameter": { "foreground": "#8C4FFF" },
    "parameter": { "foreground": "#2D6CB2" },
    "variable": { "foreground": "#2D6CB2" },
    "property": { "foreground": "#2D6CB2" },
    "enumMember": { "foreground": "#8C4FFF" },
    "event": { "foreground": "#2D6CB2" },
    "function": { "foreground": "#CC5500" },
    "method": { "foreground": "#CC5500" },
    "macro": { "foreground": "#CC5500" },
    "keyword": { "foreground": "#B22222" },
    "modifier": { "foreground": "#B22222" },
    "comment": { "foreground": "#555555" },
    "string": { "foreground": "#2E8B57" },
    "number": { "foreground": "#007B4F" },
    "regexp": { "foreground": "#007B4F" },
    "operator": { "foreground": "#333333" },
    "variable.constant": { "foreground": "#8C4FFF" },
    "variable.defaultLibrary": { "foreground": "#007B5F" }
  }
}